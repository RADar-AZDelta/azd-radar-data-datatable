{
  "name": "@radar-azdelta/svelte-datatable",
  "version": "0.0.60",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RADar-AZDelta/svelte-datatable.git"
  },
  "homepage": "https://radar-azdelta.github.io/svelte-datatable/",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev --host=127.0.0.1",
    "unlighthouse": "pnpm dlx unlighthouse --site https://radar-azdelta.github.io/svelte-datatable/",
    "build": "vite build && npm run package",
    "preview": "vite preview --host=127.0.0.1",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "pnpm run package",
    "test": "npx playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "pnpm format && prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.30.3",
    "@sveltejs/package": "^2.2.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isequal": "^4.5.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.69.6",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.1",
    "vitest": "^1.1.1"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.202",
    "arquero": "^5.4.0",
    "comlink": "^4.4.1",
    "esm-env": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0"
  },
  "type": "module"
}
